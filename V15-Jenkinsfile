pipeline {
    agent any
	tools {
	    maven "M3"
	 	}
	stages {
	    stage('Git CheckOut') {
		    steps {
			   git branch: 'dragon', url: 'https://github.com/bennetraja/rock-paper-scissors.git'
			}
		}
        stage('Clean and Install') {
            steps {
                bat 'mvn clean install'
            }
        }
        stage ('Code Quality'){
            steps {
                    withSonarQubeEnv('SonarQube Server') {
                bat 'mvn sonar:sonar'
                }
             }
        }
		
    }
}
